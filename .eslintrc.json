{
	"root": true,
	"env": {
		"browser": true
	},
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"extends": ["eslint:recommended", "prettier"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error",
		"no-implicit-coercion": "error",
		"no-unused-vars": "off",
		"no-console": "warn"
	}
}
